{
  "name": "angular-demo-app-starter",
  "version": "1.0.0",
  "description": "Angular Demo Application to show some cool stuff :-)",
  "license": "MIT",
  "jest-junit": {
    "suiteName": "angular-demo-app",
    "output": "./tmp/junit.xml"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:all": "ng serve --proxy-config config/proxy.conf.json --configuration hmr & npm run start:server",
    "start:server": "node server",
    "start:client": "ng serve --proxy-config config/proxy.conf.json --configuration hmr",
    "start:dist": "ng build --prod && DIST=true node server",
    "build": "ng build --prod",
    "build:report": "ng build --stats-json && webpack-bundle-analyzer dist/stats.json",
    "test:cli": "ng test",
    "jest": "jest --silent",
    "tdd": "jest --bail --expand --verbose",
    "tdd:cover": "jest --coverage --no-cache",
    "test": "ng test",
    "test:cover": "jest --silent --coverage --no-cache && opn coverage/index.html",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:watch": "jest --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.11",
    "@angular/cdk": "7.3.6",
    "@angular/common": "7.2.11",
    "@angular/compiler": "7.2.11",
    "@angular/core": "7.2.11",
    "@angular/forms": "7.2.11",
    "@angular/http": "7.2.11",
    "@angular/material": "7.3.6",
    "@angular/platform-browser": "7.2.11",
    "@angular/platform-browser-dynamic": "7.2.11",
    "@angular/router": "7.2.11",
    "core-js": "^2.6.5",
    "rxjs": "6.4.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.7",
    "@angular/cli": "7.3.7",
    "@angular/compiler-cli": "7.2.11",
    "@angular/language-service": "7.2.11",
    "@angularclass/hmr": "2.1.3",
    "@types/core-js": "2.5.0",
    "@types/jasmine": "~3.3.12",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~11.12.0",
    "body-parser": "1.18.3",
    "codelyzer": "^4.5.0",
    "compression": "1.7.4",
    "express": "4.16.4",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "24.5.0",
    "jest-preset-angular": "6.0.2",
    "karma": "~4.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-mocha-reporter": "^2.2.5",
    "material-design-icons": "3.0.1",
    "opn-cli": "4.0.0",
    "protractor": "~6.0.0",
    "ts-node": "~8.0.3",
    "tslint": "~5.14.0",
    "typescript": "3.2.4",
    "webpack-bundle-analyzer": "3.1.0"
  }
}
