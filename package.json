{
  "name": "angular-demo-app-starter",
  "version": "1.0.0",
  "description": "Angular Demo Application to show some cool stuff :-)",
  "license": "MIT",
  "jest-junit": {
    "suiteName": "angular-demo-app",
    "output": "./tmp/junit.xml"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:all": "ng serve --proxy-config config/proxy.conf.json --configuration hmr & npm run start:server",
    "start:server": "node server",
    "start:client": "ng serve --proxy-config config/proxy.conf.json --configuration hmr",
    "start:dist": "DIST=true node server",
    "build": "ng build --prod",
    "build:de": "ng build --prod --configuration=production:de",
    "build:report": "ng build --stats-json && webpack-bundle-analyzer dist/stats.json",
    "test:cli": "ng test",
    "jest": "jest --silent",
    "tdd": "jest --bail --expand --verbose",
    "tdd:cover": "jest --coverage --no-cache",
    "test": "ng test",
    "test:cover": "jest --silent --coverage --no-cache && open-cli coverage/index.html",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:watch": "jest --watch",
    "i18n:extract": "ng xi18n --output-path locale",
    "i18n:extract:xmb": "ng xi18n --output-path locale  --i18n-format=xmb",
    "ngx:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/{en,de}.json --clean --sort --format namespaced-json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.15",
    "@angular/cdk": "7.3.7",
    "@angular/common": "7.2.15",
    "@angular/compiler": "7.2.15",
    "@angular/core": "7.2.15",
    "@angular/forms": "7.2.15",
    "@angular/http": "7.2.15",
    "@angular/material": "7.3.7",
    "@angular/platform-browser": "7.2.15",
    "@angular/platform-browser-dynamic": "7.2.15",
    "@angular/router": "7.2.15",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "core-js": "^2.6.5",
    "messageformat": "2.2.1",
    "ngx-translate-messageformat-compiler": "4.4.0",
    "rxjs": "6.5.2",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.9",
    "@angular/cli": "7.3.9",
    "@angular/compiler-cli": "7.2.15",
    "@angular/language-service": "7.2.15",
    "@angularclass/hmr": "2.1.3",
    "@biesbjerg/ngx-translate-extract": "2.3.4",
    "@types/core-js": "2.5.0",
    "@types/jasmine": "~3.3.12",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~12.0.2",
    "body-parser": "1.19.0",
    "codelyzer": "^4.5.0",
    "compression": "1.7.4",
    "express": "4.17.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "24.8.0",
    "jest-preset-angular": "6.0.2",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-mocha-reporter": "^2.2.5",
    "material-design-icons": "3.0.1",
    "open-cli": "5.0.0",
    "protractor": "~5.4.2",
    "ts-node": "~8.1.0",
    "tslint": "~5.16.0",
    "typescript": "3.2.4",
    "webpack-bundle-analyzer": "3.3.2"
  }
}
